# =========================
# SOLVER AI ‚Äî –ë–´–°–¢–†–´–ï –†–ï–®–ï–ù–ò–Ø
# =========================

import re

# -------------------------
# –í–°–ü–û–ú–û–ì–ê–¢–ï–õ–¨–ù–û–ï
# -------------------------

def normalize(text):
    text = text.lower()
    replaces = {
        "–∫–∞–¥": "–∫–æ–¥",
        "–≤—ã–≤–∏—Å—Ç–∏": "–≤—ã–≤–µ—Å—Ç–∏",
        "–ø–µ—Ä–µ–º–µ–Ω—ã—Ö": "–ø–µ—Ä–µ–º–µ–Ω–Ω—ã—Ö",
        "–±–æ–ª—å—à–µ–µ": "–±–æ–ª—å—à–µ",
        "–º–µ–Ω—à–µ": "–º–µ–Ω—å—à–µ",
        "—Ä–∞–≤–Ω–æ": "—Ä–∞–≤–Ω—ã"
    }
    for k, v in replaces.items():
        text = text.replace(k, v)
    return text

# -------------------------
# –†–ï–®–ï–ù–ò–Ø –ß–ê–°–¢–´–• –ó–ê–î–ê–ß
# -------------------------

def solve_print(text):
    m = re.search(r"–≤—ã–≤–µ—Å—Ç–∏\s+(\d+)", text)
    if m:
        n = m.group(1)
        return f"print({n})"

def solve_if_ab(text):
    if "a –±–æ–ª—å—à–µ b" in text or "a > b" in text:
        return (
            "a = int(input())\n"
            "b = int(input())\n"
            "if a > b:\n"
            "    print(a)\n"
            "elif b > a:\n"
            "    print(b)\n"
            "else:\n"
            "    print('=')"
        )

def solve_sum(text):
    if "—Å—É–º–º" in text:
        return (
            "a = int(input())\n"
            "b = int(input())\n"
            "print(a + b)"
        )

def solve_for(text):
    if "—Ü–∏–∫–ª for" in text or "for" in text:
        return (
            "for i in range(5):\n"
            "    print(i)"
        )

def solve_while(text):
    if "while" in text:
        return (
            "i = 0\n"
            "while i < 5:\n"
            "    print(i)\n"
            "    i += 1"
        )

def solve_input(text):
    if "–≤–≤–µ—Å—Ç–∏" in text:
        return "x = input()\nprint(x)"

# -------------------------
# –ì–õ–ê–í–ù–´–ô –†–ï–®–ê–¢–ï–õ–¨
# -------------------------

def solve_task(user_text):
    text = normalize(user_text)

    for solver in [
        solve_print,
        solve_if_ab,
        solve_sum,
        solve_for,
        solve_while,
        solve_input
    ]:
        result = solver(text)
        if result:
            return result

    return None

# -------------------------
# –î–ò–ê–õ–û–ì
# -------------------------

def solver_dialog():
    print("\nüß† –û–ø–∏—à–∏ –∑–∞–¥–∞—á—É –æ–±—ã—á–Ω—ã–º–∏ —Å–ª–æ–≤–∞–º–∏:")
    user = input(">> ")

    solution = solve_task(user)

    if not solution:
        print("‚ùå –Ø –Ω–µ –ø–æ–Ω—è–ª –∑–∞–¥–∞—á—É.")
        print("–ü–æ–ø—Ä–æ–±—É–π –æ–ø–∏—Å–∞—Ç—å –ø—Ä–æ—â–µ.")
        return

    print("\n‚úÖ –ì–æ—Ç–æ–≤–æ–µ —Ä–µ—à–µ–Ω–∏–µ:\n")
    print(solution)

    print("\n‚ñ∂ –ü—Ä–∏–º–µ—Ä –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è:")
    try:
        exec(solution)
    except Exception as e:
        print("‚ö† –û—à–∏–±–∫–∞ –ø—Ä–∏ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–∏:", e)
